{
	"name": "applitrace",
	"version": "1.0.0",
	"engines": {
		"node": "12.13.0"
	},
	"description": "AppliTrace is a web application for tracking the status of work applications during the job search.",
	"main": "server/src/index.js",
	"scripts": {
		"test": "jest",
		"dev-server": "nodemon --exec babel-node ./server/server.js --ignore dist/",
		"dev-bundle": "webpack -wd",
		"dev": "webpack --mode development && babel-node server.js",
		"clean": "rm -rf build && mkdir build",
		"build-server": "babel -d ./build ./server -s",
		"start": "node ./build/server.js",
		"build": "webpack --mode production && npm run clean && npm run build-server",
		"heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install && npm run build"
	},
	"keywords": [ "job", "applications", "job applications", "application trace", "find work", "find job" ],
	"author": "Luis Riveron",
	"license": "ISC",
